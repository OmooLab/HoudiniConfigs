{
  "requires": "omoolab_env",
  "enable": "houdini_os == 'windows' and houdini_version >= '19.5'",
  "env": [
    { "OMOOLAB_OMOOLAB": "$OMOOLAB_NAS/OmooLab" },
    { "OMOOLAB_PROJECTS": "$OMOOLAB_NAS/Projects" },
    { "OMOOLAB_RESOUCES": "$OMOOLAB_NAS/Resouces" },
    { "OMOOLAB_RND": "$OMOOLAB_NAS/RnD" },
    { "OMOOLAB_PREFERS": "$OMOOLAB_CONFIG/preferences" },
    { "HOUDINI_SPLASH_FILE": "$OMOOLAB_CONFIG/splash.png" },
    { "HOUDINI_OTLSCAN_PATH": "$OMOOLAB_CONFIG/hdas" },
    { "HOUDINI_MENU_PATH": "$OMOOLAB_PREFERS/menu" },
    { "HOUDINI_BACKUP_FILENAME": "$BASENAME/$BASENAME_bak$N" },
    { "OCIO": "$HFS/houdini/ocio/config-aces-cg.ocio" },
    { "PYTHONPATH": "$OMOOLAB_CONFIG/venv/Lib/site-packages" }
  ],
  "hpath": "$OMOOLAB_PREFERS"
}
